Algoritmo "exercicio 1"

Var
   salario, novo: real
   n: caracter


Inicio
      escreval("digite o seu nome")
      leia (n)
      escreval("digite o seu salario")
      leia (salario)
      novo <- salario * 1.285
      escreval("senhor(a) ", n, " seu novo salario e: ", novo)

      

      

      



Fimalgoritmo